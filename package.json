{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:console": "concurrently \"npm run console:listen\" \"npm run dev\" --names \"CONSOLE,DEV\" --prefix-colors \"cyan,green\"",
    "dev:only": "next dev -p 3000",
    "dev:with-console": "concurrently \"npm run console:listen\" \"npm run dev:only\" --names \"CONSOLE,DEV\" --prefix-colors \"cyan,green\"",
