{
    "buildCommand": "npm run build",
    "env": {
        "SKIP_TYPE_CHECK": "1",
        "NODE_OPTIONS": "--max_old_space_size=4096"
    },
    "github": {
        "silent": true,
        "autoJobCancelation": true,
        "enabled": true
    },
    "git": {
        "deploymentEnabled": true
    },
    "ignoreCommand": "if [[ \"$VERCEL_GIT_COMMIT_REF\" == \"dependabot\"* ]]; then exit 1; fi"
}